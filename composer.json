  "autoload": {
    "psr-4": {
      "Dashifen\\ComposerScripts\\": "src"
    }
  },
  "require": {
    "php": ">=8",
    "dashifen/exception": "^1.2"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
